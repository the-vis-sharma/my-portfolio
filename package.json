  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint . --fix './src/**/*.{ts,tsx,js,jsx}'",
      "prettier --write './src/**/*.{ts,tsx,js,jsx}'",
      "stylelint --fix './src/**/*.{ts,tsx,js,jsx}'"
